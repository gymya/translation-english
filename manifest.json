{
    "manifest_version": 3,
    "name": "translateENG",
    "description": "Base Level Extension",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup/transENG.html",
        "default_icon": "img/transENG.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions":["activeTab"]
}